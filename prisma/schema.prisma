datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Url {
  id            Int         @id @default(autoincrement())
  longUrl       String      @db.Text
  shortUrl      String      @unique @db.VarChar(255)
  createdAt     DateTime    @default(now()) @db.Timestamp(6)
}